config {
  type: "table"
}

-- Simulates raw ingest view over the LIFT data for financial transactions
 SELECT
    TRIM(Ansvarsnr) AS Ansvarsnr,
    TRIM(Ansvarsnavn) as Ansvarsnavn,
    TRIM(Bare_Dag) as Bare_Dag,
    CURRENT_TIMESTAMP() AS ingestion_timestamp
    FROM
    `data_wh_dev_datapuls_cloud.raw_ref_ansvar_turnus`
    where
    trim(Ansvarsnavn) is not null